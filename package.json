{
  "name": "the-attributes",
  "version": "1.0.0",
  "description": "This is a website created to educate users about the attributes inherent in optimal performance.",
  "main": "index.html",
  "scripts": {
    "lite": "lite-server",
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -w css/scss/ -o css/",
    "watch:scss": "onchange 'css/scss/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'",
    "imagemin": "imagemin img/* -o dist/img",
    "usemin": "usemin attributes.html -d dist --htmlmin -o dist/attributes.html && usemin index.html -d dist --htmlmin -o dist/index.html",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run imagemin && npm run usemin"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "imagemin-cli": "^6.0.0",
    "lite-server": "^2.6.1",
    "node-sass": "^4.14.1",
    "onchange": "^7.1.0",
    "parallelshell": "3.0.1",
    "rimraf": "^3.0.2",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "bootstrap": "^4.6.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1"
  }
}
